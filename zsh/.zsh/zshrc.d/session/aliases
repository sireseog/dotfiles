alias ls='ls -h --color=auto'
alias ll='ls++ --potsf'
alias cp='cp -iv'
alias rm='rm -i'
alias mv='mv -i'
alias less='less --quiet'
alias grep='grep --colour=auto'
alias egrep='egrep --colour=auto'
alias fgrep='fgrep --colour=auto'
alias tree='tree -C'
alias dmesg='dmesg -deH'
alias weechat='weechat-curses'
alias top='htop'
alias fzf='fzf --multi --reverse --color=16'
alias help='run-help'
alias tmux='tmux -2'
alias workspace='cd ~/workspace'
alias todo='vim ~/workspace/TODO'
alias del='~/workspace/linux/helper/del.sh'
alias sdcard='cd /media/sdcard'
alias vim='emacsclient -t'
